        "firebase/php-jwt": "^6.10.1",
        "google/recaptcha": "^1.3",
        "tecnickcom/tcpdf": "^6.6",
        "tecnickcom/tc-lib-pdf": "^8.0",
        "setasign/fpdi": "^2.3",
        "ramsey/uuid": "^4.7",
        "mpdf/qrcode": "^1.2",
        "phpoffice/phpspreadsheet": "^4.2",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-openssl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-intl": "*"
    },
    "require-dev": {
        "matthiasmullie/minify": "^1.3",
        "spatie/ignition": "^1.12"
    }
}
