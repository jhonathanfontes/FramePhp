    "require": {
        "php": ">=8.2.0",
        "twig/twig": "^3.20.0",
        "twig/intl-extra": "^3.20.0",
        "performing/twig-components": "^0.6.1",
        "symfony/translation": "^7.0",
        "vlucas/phpdotenv": "^5.6",
        "symfony/var-dumper": "^7.0",
