{
    "name": "framephp/framework",
    "description": "Um Framework PHP simples e poderoso com funcionalidades avanÃ§adas",
    "type": "project",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        },
        "files": [
            "core/Helper/helpers.php",
            "core/Helper/functions.php",
            "core/helpers.php"
        ]
