{
    "name": "framephp/framework",
    "description": "Um Framework PHP simples e poderoso",
    "type": "project",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        },
        "files": [
            "core/Helper/helpers.php",
            "core/Helper/functions.php"
        ]
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "disable-tls": true,
        "secure-http": false
    },
    "require": {
        "php": "8.1.0",
        "twig/twig": "^3.0",
        "symfony/translation": "^6.4",
        "vlucas/phpdotenv": "^5.6",
        "symfony/var-dumper": "^6.4",
        "phpmailer/phpmailer": "^6.10"
    }
}
